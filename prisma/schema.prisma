// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  userId         Int            @id
  username       String         @unique @db.VarChar(255)
// isfriend LINE 友達追加してますか
  @@map(name: "users")
}